# Privxx Environment Variables
# Copy this file to .env and fill in the values

# Bridge service URL (override for local dev only - production uses https://privxx.app)
# VITE_BRIDGE_URL=http://localhost:8090

# App version (set at build time, e.g., "0.2.0")
VITE_APP_VERSION=0.2.0

# Build identifier (typically short git hash, e.g., "abc1234")
# Can be set in CI with: VITE_APP_BUILD=$(git rev-parse --short HEAD)
VITE_APP_BUILD=
